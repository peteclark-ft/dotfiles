#!/usr/bin/env python
# coding=utf-8

from datetime import date;
from dateutil import relativedelta;

start_date = date(2016, 05, 16)
compare_to = date.today()

delta = relativedelta.relativedelta(compare_to, start_date);

if delta.years > 0:
    print "It's been", delta.years, "years,", delta.months, "months,", delta.days, "days. ğŸ“°"
else:
    print "It's been", delta.months, "months,", delta.days, "days. ğŸ“°"

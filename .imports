#!/bin/bash
## Should import all other dotfiles. Ref'd by the bash_profile

. ~/.dotfiles/.colours
. ~/.dotfiles/.environment
. ~/.dotfiles/.aliases
. ~/.dotfiles/.ls
. ~/.dotfiles/.ssh-setup
. ~/.dotfiles/.fleet
. ~/.dotfiles/.git-helper
. ~/.dotfiles/.prompt
. ~/.dotfiles/.mongo
. ~/.dotfiles/.encryption
. ~/.dotfiles/.secret
. ~/.dotfiles/.neo
. ~/.dotfiles/.kafka
. ~/.dotfiles/.k8s
. ~/.dotfiles/.docker
. ~/.dotfiles/.vault-ft
. ~/.dotfiles/.cmdb

alias reload=". ~/.bash_profile"

[ -f /usr/local/etc/bash_completion ] && . /usr/local/etc/bash_completion
